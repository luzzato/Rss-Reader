<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="" id="net.SynRss" version="1.1.30" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="ActionSheet">
        <param name="android-package" value="nl.xservices.plugins.actionsheet.ActionSheet" />
    </feature>
    <feature name="InAppBillingPlugin">
        <param name="android-package" value="com.smartmobilesoftware.inappbilling.InAppBillingPlugin" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.liteglue.SQLitePlugin" />
    </feature>
    <icon density="ldpi" src="res/icons/android/icon-36-ldpi.png">
    </icon>
    <icon density="mdpi" src="res/icons/android/icon-48-mdpi.png">
    </icon>
    <icon density="hdpi" src="res/icons/android/icon-72-hdpi.png">
    </icon>
    <icon density="xhdpi" src="res/icons/android/icon-96-xhdpi.png">
    </icon>
    <splash density="land-hdpi" src="res/screens/android/screen-hdpi-landscape.png">
    </splash>
    <splash density="land-ldpi" src="res/screens/android/screen-ldpi-landscape.png">
    </splash>
    <splash density="land-mdpi" src="res/screens/android/screen-mdpi-landscape.png">
    </splash>
    <splash density="land-xhdpi" src="res/screens/android/screen-xhdpi-landscape.png">
    </splash>
    <splash density="port-hdpi" src="res/screens/android/screen-hdpi-portrait.png">
    </splash>
    <splash density="port-ldpi" src="res/screens/android/screen-ldpi-portrait.png">
    </splash>
    <splash density="port-mdpi" src="res/screens/android/screen-mdpi-portrait.png">
    </splash>
    <splash density="port-xhdpi" src="res/screens/android/screen-xhdpi-portrait.png">
    </splash>
    <name>SynRSS</name>
    <description>RSS reader http://SynRSS.net</description>
    <author email="dev@synrss.net" href="http://synrss.net">SynRSS</author>
    <content src="SynRSS.html" />
    <access origin="*" />
    <vs:features>
    </vs:features>
    <preference name="SplashScreen" value="screen">
    </preference>
    <preference name="windows-target-version" value="8.1">
    </preference>
    <preference name="windows-phone-target-version" value="8.1">
    </preference>
    <preference name="KeepRunning" value="True" />
    <preference name="ShowTitle" value="True" />
    <preference name="InAppBrowserStorageEnabled" value="True" />
    <preference name="SuppressesIncrementalRendering" value="True" />
    <preference name="BackupWebStorage" value="local" />
    <vs:plugin name="nl.x-services.plugins.actionsheet" version="1.1.7" />
    <vs:plugin name="cc.fovea.cordova.purchase" />
    <preference name="android-minSdkVersion" value="15" />
    <vs:plugin name="org.apache.cordova.statusbar" version="0.1.10" />
    <vs:plugin name="org.apache.cordova.dialogs" version="0.3.0" />
    <vs:plugin name="io.litehelpers.cordova.sqlite" version="0.7.10-pre" />
</widget>
